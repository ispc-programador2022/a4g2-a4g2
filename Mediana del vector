from genrdm import genrdm  #Importo la funcion para generara un vector con numeros aleatorios.


def medianardm(): #La mediana es el valor del medio, es decir con tantos numero mayores como menores en una lista ordenada de numeros.
    
    vector = genrdm() # Genero el vector con la funcion importada
    vector.sort()     # Ordeno el vector 
    
    return vector [len(vector) // 2]  #Extraigo el valor de la mitad del vector ordenado.

print(medianardm())